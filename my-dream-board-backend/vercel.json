{
  "version": 2,
  "builds": [
    { "src": "my-dream-board-backend/api/index.js", "use": "@vercel/node" },
    { "src": "my-dream-app/package.json", "use": "@vercel/static-build" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "my-dream-board-backend/api/index.js" },
    { "src": "/(.*)", "dest": "my-dream-app/$1" }
  ]
}
